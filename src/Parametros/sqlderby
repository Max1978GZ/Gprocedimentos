#sql_select_procedimientos=SELECT * FROM PROCEDIMIENTOS
#sql_select_procedimientos_where_cliente=SELECT * FROM PROCEDIMIENTOS WHERE IDCLIENTE
sql_select_find_clientes=SELECT * FROM (SELECT IDCLIENTE,CLIENTE,NUMCARPETA,DNI,DIRECCION,TELEFONO,MOVIL,INFO, (DNI || CLIENTE) AS DC  FROM CLIENTES ) AS CLI WHERE UPPER(DC) LIKE UPPER
sql_select_find_procedimientos=SELECT IDPROCEDIMIENTO,LOCALIDAD,NUMJUZGADO,PROCEDIMIENTO,PROCEDIMIENTOS.INFO,CONTRARIO,IDCLIENTE,RUTA_FICHEROS,CLIENTE,DNI FROM CLIENTES INNER JOIN PROCEDIMIENTOS ON CLIENTES.IDCLIENTE=PROCEDIMIENTOS.ID_CLIENTE WHERE UPPER(PROCEDIMIENTO) LIKE UPPER 
