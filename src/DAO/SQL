CREATE TABLE CLIENTES(
    IDCLIENTE INTEGER PRIMARY KEY GENERATED ALWAYS AS IDENTITY (START WITH 1 ,INCREMENT BY 1),
    CLIENTE VARCHAR(50),
    NUMCARPETA VARCHAR(12),
    DNI VARCHAR(12),
    DIRECCION VARCHAR(150),
    TELEFONO VARCHAR(12),
    MOVIL VARCHAR(12),    
    INFO VARCHAR(500)
);

CREATE TABLE PROCEDIMIENTOS(
    IDPROCEDIMIENTO INTEGER PRIMARY KEY GENERATED ALWAYS AS IDENTITY (START WITH 1 ,INCREMENT BY 1),
    LOCALIDAD VARCHAR(20),
    NUMJUZGADO INTEGER,
    PROCEDIMIENTO VARCHAR(20),
    INFO VARCHAR(500),
    CONTRARIO VARCHAR(100),
    ID_CLIENTE INTEGER,
    RUTA_FICHEROS VARCHAR(20),
    CONSTRAINT FK_CLIENTE_PROCEDIMIENTO FOREIGN KEY(ID_CLIENTE) REFERENCES CLIENTES (IDCLIENTE)

);

CREATE TABLE LOG(
    ID INTEGER PRIMARY KEY GENERATED ALWAYS AS IDENTITY (START WITH 1 ,INCREMENT BY 1),
    LOG VARCHAR(1000)
);


/************************** MYSQL ******************************************************/


CREATE TABLE CLIENTES(
    IDCLIENTE INTEGER PRIMARY KEY  NOT NULL AUTO_INCREMENT,
    CLIENTE VARCHAR(50),
    NUMCARPETA VARCHAR(12),
    DNI VARCHAR(12),
    DIRECCION VARCHAR(150),
    TELEFONO VARCHAR(12),
    MOVIL VARCHAR(12),    
    INFO VARCHAR(500)
);



CREATE TABLE PROCEDIMIENTOS(
    IDPROCEDIMIENTO INTEGER PRIMARY KEY NOT NULL AUTO_INCREMENT,
    LOCALIDAD VARCHAR(20),
    NUMJUZGADO INTEGER,
    PROCEDIMIENTO VARCHAR(20),
    INFO VARCHAR(500),
    CONTRARIO VARCHAR(100),
    ID_CLIENTE INTEGER,
    RUTA_FICHEROS VARCHAR(20),
    CONSTRAINT FK_CLIENTE_PROCEDIMIENTO FOREIGN KEY(ID_CLIENTE) REFERENCES CLIENTES (IDCLIENTE)

);

CREATE TABLE LOG(
    ID INTEGER PRIMARY KEY  NOT NULL AUTO_INCREMENT ,
    LOG VARCHAR(1000)
);




